parameters:
    level: max

    paths:
        - packages

    excludePaths:
        - packages/reveal-neon/tests/Generator/Source
        - packages/reveal-neon/src/Generator/local-temp
    ignoreErrors:
        - '#Method Reveal\\(.*?)Test\:\:provideData\(\) return type has no value type specified in iterable type Iterator#'

        # false positive
        - '#Parameter \#1 \$file of method PHPStan\\Rules\\RuleErrorBuilder\:\:file\(\) expects string, string\|false given#'
